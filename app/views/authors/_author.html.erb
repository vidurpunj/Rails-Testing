<div id="<%= dom_id author %>">
  <p>
    <strong>Name:</strong>
    <%= author.name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= author.age %>
  </p>

  <p>
    <strong>Dob:</strong>
    <%= author.dob %>
  </p>

  <p>
    <strong>Gender:</strong>
    <%= author.gender ? 'Male' : 'Female' %>
  </p>

</div>
